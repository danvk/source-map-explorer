{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "dist",
    "strict": true,
    "strictBindCallApply": true,
    "esModuleInterop": true,
    "noImplicitAny": false
  }
}
